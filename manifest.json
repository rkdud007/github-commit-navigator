{
  "manifest_version": 2,
  "name": "GitHub Commit Navigator",
  "version": "0.1.0",
  "description": "Navigate through commit history on GitHub using arrow keys",
  "permissions": ["https://github.com/*"],
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
